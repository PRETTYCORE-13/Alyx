                      Expanded(
                        child: Text(
                          '${venta['descProd'] ?? 'No disponible'}',
                          style: TextStyle(color: Colors.white),
                        ),
                      ),



                                      Expanded(
                  child: Text(
                    'Producto',
                    style: TextStyle(
                      color: Colors.white,
                      fontWeight: FontWeight.bold,
                    ),
                  ),
                ),
